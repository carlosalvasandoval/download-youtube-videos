# intercept request for getvideo
RewriteEngine On
RewriteRule ^.*\.mp4$ getvideo.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^\.]+)$ $1.php [NC,L]

<ifmodule mod_expires.c>
 ExpiresActive On
 ExpiresDefault A3600
 <filesmatch ".(jpg|JPG|gif|GIF|png|css|ico|js)$">
  ExpiresDefault "access plus 0 day"
  Header set Cache-Control "max-age=86400, public, must-revalidate"
  ExpiresActive On
  ExpiresByType image/jpg "access 7 day"
  ExpiresByType image/jpeg "access 7 day"
  ExpiresByType image/gif "access 7 day"
  ExpiresByType image/png "access 7 day"
  ExpiresByType text/css "access 7 day"
  ExpiresByType application/pdf "access 1 month"
  ExpiresByType application/javascript "access 1 month"
  ExpiresByType application/x-javascript "access 1 month"
  ExpiresByType application/x-shockwave-flash "access 1 month"
  ExpiresByType image/x-icon "access 1 year"
  ExpiresDefault "access 2 days"
 </filesmatch>
</ifmodule>
